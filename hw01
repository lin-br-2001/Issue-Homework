#include <stdio.h>

int main(void)
{
    unsigned int counter=1;
    int ayg;
    
    printf("Guess a number:");
    scanf("%d", &ayg);
    
    if(ayg>5){
            printf("Your ans is greater than the correct ans.\n");
        }
        if(ayg<5){
            printf("Your ans is smaller than the correct ans.\n");
        }
        if(ayg==5){
            printf("Your ans is the correct ans!\n");
        }
    
    while(counter!=0){
        printf("Guess a number:");
        scanf("%d", &ayg);
        
        counter+=1;
        
        if(ayg>5){
            printf("Your ans is greater than the correct ans.\n");
        }
        if(ayg<5){
            printf("Your ans is smaller than the correct ans.\n");
        }
        if(ayg==5){
            printf("Your ans is the correct ans!\n");
        }
    }
}
